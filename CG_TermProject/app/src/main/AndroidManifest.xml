<manifest package="com.medialab.android_gles_sample"
		  xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity
			android:name="com.medialab.android_gles_sample.MainActivity"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.medialab.android_gles_sample.sample.ColoringView"/>
		<activity android:name="com.medialab.android_gles_sample.sample.FragLightingView"/>
		<activity android:name="com.medialab.android_gles_sample.sample.NormalMappingView"/>

	</application>

</manifest>
